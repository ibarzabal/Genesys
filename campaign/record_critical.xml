<?xml version="1.0" encoding="iso-8859-1"?>
<root>

	<windowclass name="criticalsmall">
		<sizelimits>
			<maximum>
				<height>20</height>
			</maximum>
		</sizelimits>
		<sheetdata>
			<windowbutton name="open">
				<bounds>0,1,20,20</bounds>
				<class>critical</class>
			</windowbutton>
			<basestringfield name="name">
				<bounds>21,4,-1,17</bounds>
				<empty>&#171; New Critical &#187;</empty>
				<disabled />
			</basestringfield>
		</sheetdata>
	</windowclass>

	<windowclass name="critical">
		<frame>referencepage</frame>
		<placement>
			<size>
				<width>300</width>
				<height>334</height>
			</size>
		</placement>
		<minimize>minimized_reference</minimize>
		<playercontrol />
		<sharable />
		<tooltip>
			<field>name</field>
		</tooltip>
		<sheetdata>
<!-- NAME -->
			<sheetframe name="main_name">
				<bounds>14,15,275,49</bounds>
			</sheetframe>

			<sheetframetitle name="main_name_title">
				<anchored>
					<to>main_name</to>
				</anchored>
				<static>Critical</static>
			</sheetframetitle>

			<windowshortcut name="main_name_shortcut">
				<anchored>
					<to>main_name</to>
					<position>insidetopleft</position>
					<offset>17,18</offset>
					<size>
						<height>20</height>
						<width>20</width>
					</size>
				</anchored>
				<class>critical</class>
			</windowshortcut>
			<basestringfield name="main_name_name" source="name">
				<anchored>
					<to>main_name</to>
					<position>insidetopleft</position>
					<offset>38,19</offset>
					<size>
						<height>20</height>
						<width>200</width>
					</size>
				</anchored>
				<empty>&#171; New Critical &#187;</empty>
				<tabtarget>
					<next>main_details_severity</next>
					<prev>main_details_severity</prev>
				</tabtarget>
			</basestringfield>

<!-- DETAILS -->
			<sheetframe name="main_details">
				<bounds>14,61,275,52</bounds>
			</sheetframe>
			<sheetframetitle name="main_main_title">
				<anchored>
					<to>main_details</to>
				</anchored>
				<static>Details</static>
			</sheetframetitle>

			<!-- SEVERITY -->
			<sheetlabel>
				<anchored>
					<to>main_details</to>
					<offset>17,19</offset>
					<size>
						<width>209</width>
					</size>
				</anchored>
				<static>Severity</static>
			</sheetlabel>
			<sheetnumberfield name="main_details_severity" source="severity">
				<anchored>
					<to>main_details</to>
					<offset>226,19</offset>
					<size>
						<width>25</width>
					</size>
				</anchored>
				<tabtarget>
					<next>main_name_name</next>
					<prev>main_name_name</prev>
				</tabtarget>
			</sheetnumberfield>

<!-- DESCRIPTION -->
			<sheetframe name="main_description">
				<bounds>14,110,275,211</bounds>
			</sheetframe>
			<sheetframetitle name="main_description_title">
				<anchored>
					<to>main_description</to>
				</anchored>
				<static>Description</static>
			</sheetframetitle>

			<sheetformattedtextfield name="main_description_description" source="description">
				<anchored>
					<left>
						<parent>main_description</parent>
						<anchor>left</anchor>
						<offset>13</offset>
					</left>
					<right>
						<parent>main_description</parent>
						<anchor>right</anchor>
						<offset>-13</offset>
					</right>
					<top>
						<parent>main_description</parent>
						<anchor>top</anchor>
						<offset>20</offset>
					</top>
					<bottom>
						<parent>main_description</parent>
						<anchor>bottom</anchor>
						<offset>-20</offset>
					</bottom>
				</anchored>
			</sheetformattedtextfield>
			<scrollbar>
				<anchored>
					<to>main_description_description</to>
					<offset>-3,-2</offset>
				</anchored>
				<target>main_description_description</target>
			</scrollbar>

			<close_referencepage />
		</sheetdata>
	</windowclass>



		<windowclass name="critical_item1">
			<margins control="0,0,0,2" />
			<sheetdata>
				<genericcontrol name="rightanchor">
					<anchored position="insidetopright" offset="0,2" height="0" width="0" />
				</genericcontrol>
				<button_idelete name="idelete">
					<anchored width="20" height="20">
						<top offset="2" />
						<right parent="rightanchor" anchor="left" relation="relative" offset="-5" />
					</anchored>
				</button_idelete>
				<linkcontrol name="shortcut">
					<anchored to="rightanchor" width="20" height="20">
						<top />
						<right anchor="left" relation="relative" offset="-2" />
					</anchored>
					<class>critical</class>
					<readonly />
				</linkcontrol>
				<basenumberfield name="severity">
					<anchored to="rightanchor" width="20" height="20">
						<top />
						<right anchor="left" relation="relative" offset="-10" />
					</anchored>
					<disabled />
				</basenumberfield>
				<string_textlistitem name="name">
					<anchored to="rightanchor">
						<top />
						<left parent="" offset="5" />
						<right anchor="left" relation="relative" offset="-10" />
					</anchored>
					<multilinespacing>20</multilinespacing>
				</string_textlistitem>
			</sheetdata>
		</windowclass>


<windowclass name="critical_item">
	<margins control="0,0,0,2" />
	<sheetdata>
		<anchor_listitem_column_right name="rightanchor" />

		<!--			<button_listitem_column_delete name="idelete" /> -->
		<button_idelete name="idelete">
			<anchored to="rightanchor">
				<top />
				<right anchor="left" relation="relative" offset="-5" />
			</anchored>
		</button_idelete>

		<link_listitem_column name="shortcut">
			<class>critical</class>
		</link_listitem_column>
		<basenumberfield name="severity">
			<anchored to="rightanchor" width="20" height="20">
				<top />
				<right anchor="left" relation="relative" offset="-10" />
			</anchored>
			<disabled />
		</basenumberfield>
		<string_listitem_column_name name="name">
				<disabled />
		</string_listitem_column_name>
		</sheetdata>
</windowclass>

</root>
