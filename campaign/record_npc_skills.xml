					<right parent="main_statblock1_brawn_current" anchor="right" offset="20"/>
				</anchored>
				<center />
				<static>Brawn</static>
			</title_statblock>
			<!-- AGILITY-->
			<sheetcharacteristicvalue name="main_statblock1_agility_current" source="agility.current">
