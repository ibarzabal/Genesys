<root>
	<windowclass name="charsheet_vehicle">
		<sheetdata>
			<label_charframetopg name="vehicletitle">
				<bounds>15,10,-10,20</bounds>
				<icons>char_inventory</icons>
				<static textres="char_label_vehicle" />
			</label_charframetopg>

			<subwindow name="contents">
				<script>
					function OnInit()
						window.contents.setVisible(false);
					end
				</script>

				<anchored>
					<bottom offset="-40" />
					<top offset="38" />
					<left offset="8" />
					<right offset="-15" />
				</anchored>
				<class>charsheet_active_vehicle</class>
				<activate />
				<fastinit />
			</subwindow>

		</sheetdata>
	</windowclass>



	<windowclass name="charsheet_active_vehicle">
		<margins control="0,0,0,2" />
		<sheetdata>
			<anchor_column name="columnanchor" />
			<subwindow name="actions">
				<anchored>
					<top parent="columnanchor" anchor="bottom" relation="relative" offset="0" />
					<left />
					<right />
				</anchored>
				<activate />
				<fastinit />
				<class>power_page</class>

			</subwindow>
		</sheetdata>
	</windowclass>

</root>
