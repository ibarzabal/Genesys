<template name="anchor_title_charsheet">
  <genericcontrol>
    <anchored height="0">
      <top offset="35" />
      <left parent="" anchor="left" offset="15" />
      <right parent="" anchor="right" offset="-30" />
    </anchored>
    <disabled />
  </genericcontrol>
</template>
